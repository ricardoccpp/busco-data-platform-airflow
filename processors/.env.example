# Ambiente
ENVIRONMENT=development
LOG_LEVEL=INFO

# AWS
AWS_REGION=us-east-1
AWS_PROFILE=default
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=

# S3
S3_BUCKET_NAME=busco-data-lake
BRONZE_PREFIX=bronze/
SILVER_PREFIX=silver/
GOLD_PREFIX=gold/

# DuckDB
DUCKDB_MEMORY_LIMIT=8GB
DUCKDB_THREADS=4
DUCKDB_TEMP_DIR=/tmp/duckdb

# Glue
GLUE_DATABASE_PREFIX=busco_
GLUE_CREATE_TABLES=true
GLUE_UPDATE_SCHEMA=true

# ECR
ECR_REPOSITORY_URI=123456789012.dkr.ecr.us-east-1.amazonaws.com/busco-datalake-processor